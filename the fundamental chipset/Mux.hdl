/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=notsel);
    And(a=sel, b=b, out=c);
    And(a=a, b=notsel, out=d);
    Or(a=c, b=d, out=out);
}